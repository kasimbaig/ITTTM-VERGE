

<p-dialog 
  [(visible)]="isFormOpen" 
  [modal]="true" 
  [closable]="true"
  [maximizable]="true"
  [draggable]="false"
  [resizable]="true"
  [maximizable]="true"
  [baseZIndex]="10000"
  [style]="{width: '98vw', height: '95vh'}"
  (onHide)="onCancel()"
>
  <ng-template pTemplate="header">
    <div class="dialog-header-content">
      <h2 class="dialog-title">{{ dialogTitle }}</h2>
    </div>
  </ng-template>

  <div class="p-4 bg-gray-50 overflow-y-auto h-full">
    <div class="w-full">
    
    <form [formGroup]="gtgForm" (ngSubmit)="onSubmit()" class="w-full space-y-4">
      <!-- Header Section -->
      <div class="bg-gradient-to-r from-blue-50 to-indigo-50 shadow-sm border border-gray-200 mb-4">
        <div class="p-4">
          <!-- Top Title -->
          <div class="text-center mb-4">
            <h1 class="text-2xl font-bold text-gray-800 uppercase">LOAD TRIALS - GTG</h1>
          </div>

          <!-- Trial Information Fields -->
          <div class="mb-4 space-y-3">
            <div class="flex items-center justify-center">
              <span class="text-lg font-semibold text-gray-700">SHIP</span>
              <div class="border-b-2 border-gray-400 mx-2">
                <select formControlName="ship" (change)="onShipChange($event)" class="w-32 px-2 py-1 border border-black focus:outline-none focus:ring-2 focus:ring-blue-500 text-center bg-transparent ship-dropdown" [disabled]="isReadonly" [class.disabled]="isReadonly">
                  <option value="">Select Ship</option>
                  <option *ngFor="let ship of ships" [value]="ship.id">{{ ship.name }}</option>
                </select>
              </div>
            </div>
            <div class="flex items-center justify-center">
              <span class="text-base font-medium text-gray-700 mr-4">OCCASION OF TRIALS -</span>
              <div class="border-b-2 border-gray-400 flex-1 max-w-md">
                <input type="text" formControlName="occasionOfTrials" class="w-full px-2 py-1 border border-black focus:outline-none focus:ring-2 focus:ring-blue-500" placeholder="Enter occasion of trials" [readonly]="isReadonly">
              </div>
            </div>
            
            <div class="flex items-center justify-center">
              <span class="text-base font-medium text-gray-700 mr-4">TRIAL DATE -</span>
              <div class="border-b-2 border-gray-400 flex-1 max-w-md">
                <input type="date" formControlName="trial_date" class="w-full px-2 py-1 border border-black focus:outline-none focus:ring-2 focus:ring-blue-500" [readonly]="isReadonly">
              </div>
            </div>
          </div>

          <!-- Main Report Header -->
          <div class="text-center mb-4">
            <h2 class="text-xl font-bold text-gray-800 uppercase mb-2">GAS TURBINE GENERATOR</h2>
            <div class="flex items-center justify-center mb-2">
              <span class="text-lg font-semibold text-gray-700">LOAD TRIAL REPORT - GTG</span>
            </div>
          </div>
        </div>
      </div>
      
      <!-- Route Configuration Section - Only show in edit mode -->
      <div *ngIf="!isAddMode && reportData?.id" class="bg-white shadow-sm border border-gray-200">
        <div class="p-6">
          <app-route-config 
            [useApi]="true"
            [transactionId]="reportData?.id"
            [submodule]="5"
            [vesselId]="selectedShipId ? selectedShipId : undefined"
            [formTitle]="'GTG Load Trial Report Route Config'"
            [hideButtons]="isViewMode"
            (routeConfigSaved)="onRouteConfigSaved($event)"
            (nextStepClicked)="onNextStep($event)"
            (timelineToggled)="onTimelineToggle($event)">
          </app-route-config>
        </div>
      </div>
    <div id="printContainer" class="p-2" >
      <!-- Section 1: Trial Details -->
      <div class="bg-white shadow-sm border border-gray-200">
        <div class="px-4 py-3 border-b border-gray-200 bg-[#1e40af] heading">
          <h2 class="text-lg font-semibold text-white">1. Trial Details</h2>
        </div>
        
        <div class="p-4 bg-amber-50">
          <div class="grid grid-cols-1 lg:grid-cols-2 gap-4">
            
            <!-- Presented By -->
            <div class="space-y-1">
              <label class="block text-base font-medium text-gray-700">
                (a) Presented by<span class="text-red-500">*</span>
              </label>
              <input 
                type="text"
                formControlName="presentedBy"
                class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                [class.border-red-500]="isFieldInvalid('presentedBy')"
                [readonly]="isReadonly"
                placeholder="Enter presenter name">
            </div>

            <!-- Trial Date -->
            <div class="space-y-1">
              <label class="block text-base font-medium text-gray-700">
                (b) Trial date<span class="text-red-500">*</span>
              </label>
              <input 
                type="date"
                formControlName="trialDate"
                class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                [class.border-red-500]="isFieldInvalid('trialDate')"
                [readonly]="isReadonly">
            </div>

            <!-- Occasion of Current Trial -->
            <div class="space-y-1">
              <label class="block text-base font-medium text-gray-700">
                (c) Occasion of current trial<span class="text-red-500">*</span>
              </label>
              <input 
                type="text"
                formControlName="occasionOfCurrentTrial"
                class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                [class.border-red-500]="isFieldInvalid('occasionOfCurrentTrial')"
                [readonly]="isReadonly"
                placeholder="Enter occasion">
            </div>

            <!-- Date of Last Trial -->
            <div class="space-y-1">
              <label class="block text-base font-medium text-gray-700">
                (d) Date of last trial carried out on<span class="text-red-500">*</span>
              </label>
              <input 
                type="date"
                formControlName="dateOfLastTrial"
                class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                [class.border-red-500]="isFieldInvalid('dateOfLastTrial')"
                [readonly]="isReadonly">
            </div>

            <!-- Proposal Reference -->
            <div class="space-y-1">
              <label class="block text-base font-medium text-gray-700">
                (e) Proposal reference<span class="text-red-500">*</span>
              </label>
              <input 
                type="text"
                formControlName="proposalReference"
                class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                [class.border-red-500]="isFieldInvalid('proposalReference')"
                [readonly]="isReadonly"
                placeholder="Enter proposal reference">
            </div>

            <!-- File Reference -->
            <div class="space-y-1">
              <label class="block text-base font-medium text-gray-700">
                (f) File reference<span class="text-red-500">*</span>
              </label>
              <input 
                type="text"
                formControlName="fileReference"
                class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                [class.border-red-500]="isFieldInvalid('fileReference')"
                [readonly]="isReadonly"
                placeholder="Enter file reference">
            </div>

            <!-- Reference Document -->
            <div class="lg:col-span-2 space-y-1">
              <label class="block text-base font-medium text-gray-700">
                (g) Reference document for trial:
              </label>
              <input 
                type="text"
                formControlName="referenceDocument"
                class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                placeholder="Enter reference document">
            </div>
          </div>
        </div>
      </div>

      <!-- Section 2: Test Equipment Used -->
      <div class="bg-white shadow-sm border border-gray-200">
        <div class="px-4 py-3 border-b border-gray-200 bg-[#1e40af] heading">
          <h2 class="text-lg font-semibold text-white">2. Test Equipment Used</h2>
        </div>
        
        <div class="p-4 bg-amber-50">
          <select 
            formControlName="equipmentId"
            class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
            [disabled]="isReadonly || isLoadingEquipments">
            <option value="">Select Equipment</option>
            <option *ngFor="let equipment of equipments" [value]="equipment.id">
              {{ equipment.name }}
            </option>
          </select>
          <div *ngIf="isLoadingEquipments" class="mt-2 text-sm text-gray-500">
            Loading equipments...
          </div>
        </div>
      </div>

      <!-- Section 3: Equipment Details -->
      <div class="bg-white shadow-sm border border-gray-200">
        <div class="px-4 py-3 border-b border-gray-200 bg-[#1e40af] heading">
          <h2 class="text-lg font-semibold text-white">3. Equipment Details</h2>
        </div>
        
        <div class="p-4 space-y-6">
          
          <!-- Engine Subsection -->
          <div class="border border-gray-300 bg-amber-50 rounded-lg p-4">
            <h3 class="text-lg font-semibold text-gray-800 mb-3 border-b border-gray-300 pb-2">Engine</h3>
            <div class="grid grid-cols-1 lg:grid-cols-3 gap-4">
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (a) Make<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="engineMake"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('engineMake')"
                  [readonly]="isReadonly"
                  placeholder="Enter engine make">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (b) Model & serial no.<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="engineModelSerial"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('engineModelSerial')"
                  [readonly]="isReadonly"
                  placeholder="Enter model and serial number">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (c) RPM<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="engineRpm"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('engineRpm')"
                  [readonly]="isReadonly"
                  placeholder="Enter RPM">
              </div>
            </div>
          </div>

          <!-- Governor Subsection -->
          <div class="border border-gray-300 bg-amber-50 rounded-lg p-4">
            <h3 class="text-lg font-semibold text-gray-800 mb-3 border-b border-gray-300 pb-2">Governor</h3>
            <div class="grid grid-cols-1 lg:grid-cols-3 gap-4">
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (a) Make<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="governorMake"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('governorMake')"
                  [readonly]="isReadonly"
                  placeholder="Enter governor make">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (b) Model & serial no.<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="governorModelSerial"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('governorModelSerial')"
                  [readonly]="isReadonly"
                  placeholder="Enter model and serial number">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (c) Type<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="governorType"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('governorType')"
                  [readonly]="isReadonly"
                  placeholder="Enter governor type">
              </div>
            </div>
          </div>

          <!-- Alternator Subsection -->
          <div class="border border-gray-300 bg-amber-50 rounded-lg p-6">
            <h3 class="text-lg font-semibold text-gray-800 mb-4 border-b border-gray-300 pb-2">Alternator</h3>
            <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-3 gap-6">
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (a) Make and rating<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorMakeRating"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorMakeRating')"
                  [readonly]="isReadonly"
                  placeholder="Enter make and rating">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (b) Model & serial no.<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorModelSerial"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorModelSerial')"
                  [readonly]="isReadonly"
                  placeholder="Enter model and serial number">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (c) Type<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorType"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorType')"
                  [readonly]="isReadonly"
                  placeholder="Enter alternator type">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (d) Rated voltage<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorRatedVoltage"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorRatedVoltage')"
                  [readonly]="isReadonly"
                  placeholder="Enter rated voltage">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (e) Rated frequency<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorRatedFrequency"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorRatedFrequency')"
                  [readonly]="isReadonly"
                  placeholder="Enter rated frequency">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (f) Rated kVA/ kW<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorRatedKvaKw"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorRatedKvaKw')"
                  [readonly]="isReadonly"
                  placeholder="Enter rated kVA/kW">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (g) Rated current<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorRatedCurrent"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorRatedCurrent')"
                  [readonly]="isReadonly"
                  placeholder="Enter rated current">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (h) Bearing number<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="alternatorBearingNumber"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('alternatorBearingNumber')"
                  [readonly]="isReadonly"
                  placeholder="Enter bearing number">
              </div>
            </div>
          </div>

          <!-- AVR Subsection -->
          <div class="border border-gray-300 bg-amber-50 rounded-lg p-6">
            <h3 class="text-lg font-semibold text-gray-800 mb-4 border-b border-gray-300 pb-2">AVR</h3>
            <div class="grid grid-cols-1 md:grid-cols-2 gap-6">
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (a) Make and type<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="avrMakeType"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('avrMakeType')"
                  [readonly]="isReadonly"
                  placeholder="Enter make and type">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (b) Model & serial no.<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="avrModelSerial"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('avrModelSerial')"
                  [readonly]="isReadonly"
                  placeholder="Enter model and serial number">
              </div>
            </div>
          </div>

          <!-- Supply Breaker Subsection -->
          <div class="border border-gray-300 bg-amber-50 rounded-lg p-6">
            <h3 class="text-lg font-semibold text-gray-800 mb-4 border-b border-gray-300 pb-2">Supply Breaker</h3>
            <div class="grid grid-cols-1 md:grid-cols-3 gap-6">
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (a) Make<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="supplyBreakerMake"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('supplyBreakerMake')"
                  [readonly]="isReadonly"
                  placeholder="Enter supply breaker make">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (b) Model & serial no.<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="supplyBreakerModelSerial"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('supplyBreakerModelSerial')"
                  [readonly]="isReadonly"
                  placeholder="Enter model and serial number">
              </div>
              <div class="space-y-1">
                <label class="block text-base font-medium text-gray-700">
                  (c) Rated capacity (Amps)<span class="text-red-500">**</span>
                </label>
                <input 
                  type="text"
                  formControlName="supplyBreakerRatedCapacity"
                  class="w-full px-3 py-2 border border-black shadow-sm focus:outline-none focus:ring-2 focus:ring-blue-500 focus:border-blue-500"
                  [class.border-red-500]="isFieldInvalid('supplyBreakerRatedCapacity')"
                  [readonly]="isReadonly"
                  placeholder="Enter rated capacity in Amps">
              </div>
            </div>
          </div>
        </div>
      </div>

      <!-- Section 4: Insulation Resistance -->
      <div class="bg-white shadow-sm border border-gray-200">
        <div class="px-6 py-4 border-b border-gray-200 bg-[#1e40af] heading ">
          <h2 class="text-lg font-semibold text-white">4. Insulation Resistance (iaw NES 511(Issue 2) and NES 502 issue 4)</h2>
        </div>
        
        <div class="p-6">
          <div class="overflow-x-auto">
            <table class="w-full border-collapse border border-gray-400 bg-amber-50">
              <thead>
                <tr class="bg-gray-100">
                  <th class="border border-gray-400 px-4 py-2 text-left font-semibold">Item</th>
                  <th class="border border-gray-400 px-4 py-2 text-center font-semibold">Value</th>
                </tr>
              </thead>
              <tbody>
                <tr>
                  <td class="border border-gray-400 px-4 py-2">(a) Generator hot (>1MΩ)<span class="text-red-500">*</span></td>
                  <td class="border border-gray-400 px-4 py-2">
                    <input type="text" formControlName="generatorHot1" class="w-full px-2 py-1 border border-black focus:outline-none focus:ring-2 focus:ring-blue-500" [readonly]="isReadonly" placeholder="Enter value">
                  </td>
                </tr>
                <tr>
                  <td class="border border-gray-400 px-4 py-2">(b) Generator cold (>1MΩ)<span class="text-red-500">*</span></td>
                  <td class="border border-gray-400 px-4 py-2">
                    <input type="text" formControlName="generatorCold1" class="w-full px-2 py-1 border border-black focus:outline-none focus:ring-2 focus:ring-blue-500" [readonly]="isReadonly" placeholder="Enter value">
                  </td>
                </tr>
                <tr>
                  <td class="border border-gray-400 px-4 py-2">(c) Switchboard (>2MΩ)<span class="text-red-500">*</span></td>
                  <td class="border border-gray-400 px-4 py-2">
                    <input type="text" formControlName="switchboard1" class="w-full px-2 py-1 border border-black focus:outline-none focus:ring-2 focus:ring-blue-500" [readonly]="isReadonly" placeholder="Enter value">
                  </td>
                </tr>
                <tr>
                  <td class="border border-gray-400 px-4 py-2">(d) Generator to switchboard cable (>20MΩ)<span class="text-red-500">*</span></td>
                  <td class="border border-gray-400 px-4 py-2">
                    <input type="text" formControlName="generatorCable1" class="w-full px-2 py-1 border border-black focus:outline-none focus:ring-2 focus:ring-blue-500" [readonly]="isReadonly" placeholder="Enter value">
                  </td>
                </tr>
                <tr>
                  <td class="border border-gray-400 px-4 py-2">(e) Insulation breaker (>10MΩ)<span class="text-red-500">*</span></td>
